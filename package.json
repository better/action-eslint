{
  "name": "lint-diff-gihub-action",
  "version": "1.0.0",
  "private": true,
  "description": "GitHub Action to run ESLint",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/node12-template.git"
  },
  "keywords": [
    "actions",
    "diff",
    "eslint",
    "github",
    "linter",
    "node"
  ],
  "author": "Konstantin Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.0",
    "@actions/github": "2.1.0",
    "commander": "^2.15.1",
    "eslint": "6.8.0",
    "execa": "^0.10.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@octokit/rest": "16.38.2",
    "@types/eslint": "6.1.7",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.17",
    "@types/semver": "6.2.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-ramda": "^1.6.1",
    "eslint-config-pagarme-base": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "jest": "25.1.0",
    "jest-circus": "25.1.0",
    "ts-jest": "25.0.0",
    "typescript": "3.7.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "script"
    },
    "extends": "eslint:recommended"
  }
}
